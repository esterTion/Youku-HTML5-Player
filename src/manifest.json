{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.2.5.5",
  "description": "__MSG_extDesc__",
  "content_scripts": [
    {
      "matches": [
        "*://v.youku.com/v_show/*",
        "*://player.youku.com/embed/*"
      ],
      "all_frames": true,
      "js": [
        "dom_gen.js",
        "resizeSensor.js",
        "md5.js",
        "hookFetch.js",
        "flv.min.js",
        "jquery-2.1.4.min.js",
        "google-style-loading.min.js",
        "CommentCoreLibrary.min.js",
        "biliplus_shield.min.js",
        "ABPlayer.min.js",
        "youku_html5.js"
      ],
      "css": [
        "ABPlayer.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*/*"
      ],
      "exclude_matches": [
        "*://v.youku.com/v_show/*",
        "*://player.youku.com/embed/*"
      ],
      "all_frames": true,
      "js": [
        "dom_gen.js",
        "youku_embed_replacer.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://v.youku.com/v_show/*"
      ],
      "all_frames": true,
      "js": [
        "official_html5_enabler.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://v-wb.youku.com/*"
      ],
      "all_frames": true,
      "js": [
        "v_slash_jumper.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "hookFetch_background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  },
  "permissions": [
    "*://*/*",
    "storage"
  ],
  "icons": {
    "48": "icon.png"
  },
  "default_locale": "en",
  "options_ui": {
    "page": "options.html"
  },
  "web_accessible_resources": [
    "options.html"
  ],
  "minimum_chrome_version": "49.0"
}